{
    "expo": {
        "name": "Listas Coquette",
        "slug": "coquette-list",
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "./assets/icon-listas.png",
        "userInterfaceStyle": "light",
        "newArchEnabled": false,
        "splash": {
            "image": "./assets/splash-listas.png",
            "resizeMode": "contain",
            "backgroundColor": "#FACCD6"
        },
        "scheme": "coquette-list",
        "ios": {
            "supportsTablet": false,
            "bundleIdentifier": "com.paulaymanu113.coquettelist"
        },
        "android": {
            "adaptiveIcon": {
                "foregroundImage": "./assets/icon-listas.png",
                "backgroundColor": "#FACCD6"
            },
            "versionCode": 13,
            "package": "com.paulaymanu113.coquettelist"
        },
        "locales": {
            "en": "./languages/english.json",
            "ar": "./languages/arab.json",
            "de": "./languages/deutsch.json",
            "hi": "./languages/hindi.json",
            "fr": "./languages/french.json",
            "id": "./languages/indonesian.json",
            "pt": "./languages/portuguese.json",
            "ru": "./languages/russian.json",
            "pl": "./languages/polish.json",
            "vi": "./languages/vietnamese.json",
            "tr": "./languages/turkish.json"
        },
        "web": {
            "favicon": "./assets/favicon.png"
        },
        "plugins": [
            [
                "react-native-google-mobile-ads",
                {
                    "androidAppId": "ca-app-pub-3738413299329691~1309052823",
                    "iosAppId": "ca-app-pub-3738413299329691~1309052823",
                    "delayAppMeasurementInit": true,
                    "userTrackingUsageDescription": "This identifier will be used to deliver personalized ads to you."
                }
            ],
            [
                "expo-build-properties",
                {
                    "android": {
                        "extraProguardRules": "-keep class com.google.android.gms.internal.consent_sdk.** { *; }"
                    }
                }
            ],
            "expo-font",
            "expo-router",
            "expo-localization"
        ],
        "extra": {
            "eas": {
                "projectId": "b6e568e1-c05e-425f-82aa-8ef92b9c27f3"
            }
        }
    }
}